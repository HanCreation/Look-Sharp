# Database
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/looksharp"
# For local development without PostgreSQL, leave empty to use SQLite

# Google AI Studio
GEMINI_API_KEY="your_gemini_api_key_here"
GEMINI_MODEL_ID="gemini-2.5-flash-image-preview"

# Vercel Blob Storage (optional - will fallback to local storage if not set)
BLOB_READ_WRITE_TOKEN="your_vercel_blob_token_here"

# Upload limits
MAX_UPLOAD_MB=10

# Development
NODE_ENV="development"